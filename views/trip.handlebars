<div class="content-body">

     
            <!-- row -->

            <div class="container-fluid">

	<div class="container-contact100">
		<div class="wrap-contact100">
		<!-- ithe form chi action takjo je rajat sangel -->
			<form class="contact100-form validate-form" action="/register/order" method="post">
				<span class="contact100-form-title">
					Trip Description
				</span>

				<div class="wrap-input100 input100-select">
					<span class="label-input100">From</span>
					<div>
						<select class="selection-2" name="source_id">
							<option>Source</option>
							{{#if cities}}
                                {{#each cities}}
                                    <option value="{{this.id}}">{{this.city}}</option>
                                {{/each}}
                            {{/if}}
							
						</select>
					</div>
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 input100-select">
					<span class="label-input100">To</span>
					<div>
						<select class="selection-2" name="destination_id">
							<option>Destination</option>
							{{#if cities}}
                                {{#each cities}}
                                    <option value="{{this.id}}">{{this.city}}</option>
                                {{/each}}
                            {{/if}}
						</select>
					</div>
					<span class="focus-input100"></span>
				</div>

                <div class="wrap-input100 input100-select">
					<span class="label-input100">Product</span>
					<div>
						<select class="selection-2" name="product_id">
							<option>Product</option>
							{{#if products}}
                                {{#each products}}
                                    <option value="{{this.id}}">{{this.name}}</option>
                                {{/each}}
                            {{/if}}
						</select>
					</div>
					<span class="focus-input100"></span>
				</div>

                <div class="wrap-input100 input100-select">
					<span class="label-input100">Delivery</span>
					<div>
						<select class="selection-2" name="delivery_id">
							<option>Delivery</option>
							{{#if deliver}}
                                {{#each deliver}}
                                    <option value="{{this.id}}">{{this.vehicle_no}}</option>
                                {{/each}}
                            {{/if}}
						</select>
					</div>
					<span class="focus-input100"></span>
				</div>
		

				<div class="container-contact100-form-btn">
					<div class="wrap-contact100-form-btn">
						<div class="contact100-form-bgbtn"></div>
						<button class="contact100-form-btn">
							<span>
								Submit
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
						</button>
					</div>
				</div>
			</form>
		</div>
	</div>
	
	<div id="dropDownSelect1"></div>

            </div>
            <!-- #/ container -->
        </div>