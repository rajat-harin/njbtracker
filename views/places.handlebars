

        <div class="container-contact100">
            <div class="wrap-contact100">
                <form method="POST" class="contact100-form validate-form" action="/register/places" >
                    <span class="contact100-form-title">
                        Locations details
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Area is required">
                        <span class="label-input100">Area</span>
                        <input class="input100" type="textarea" name="area" placeholder="Enter full address">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="placetype is required">
                        <span class="label-input100">Place Type</span>
                        <input class="input100" type="text" name="place_type" placeholder="Enter place type">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 input100-select">
                        <span class="label-input100">Select the country</span>
                        <div>
                            <select class="selection-2" name="country">
                                <option>Select the country</option>
                                <option>India</option>
                                <option>USA</option>
                                <option>UK</option>
                            </select>
                        </div>
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 input100-select">
                        <span class="label-input100">State</span>
                        <div>
                            <select class="selection-2" name="state">
                                <option>Select the state</option>
                                <option>Maharashtra</option>
                                <option>Haryana</option>
                                <option>Jharkhand</option>
                            </select>
                        </div>
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 input100-select">
                        <span class="label-input100">City</span>
                        <div>
                            <select class="selection-2" name="city">
                                <option>Select the city</option>
                                <option>Nagpur</option>
                                <option>Delhi</option>
                                <option>Mumbai</option>
                            </select>
                        </div>
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Pincode is required">
                        <span class="label-input100">Pincode</span>
                        <input class="input100" type="text" name="pincode" placeholder="Enter pincode">
                        <span class="focus-input100"></span>
                    </div>


                    <div class="container-contact100-form-btn">
                        <div class="wrap-contact100-form-btn">
                            <div class="contact100-form-bgbtn"></div>
                            <button class="contact100-form-btn">
                                <span>
                                    Submit
                                    <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                                </span>
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div id="dropDownSelect1"></div>
