	<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action="/driverreg" method="post">
				<span class="contact100-form-title">
					New Driver Registration
				</span>

				<div class="wrap-input100 validate-input" data-validate="Driver's Name is required">
					<span class="label-input100">Driver's Name</span>
					<input class="input100" type="text" name="drivername" placeholder="Enter driver's name">
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Driver id is required">
					<span class="label-input100">Driver ID</span>
					<input class="input100" type="text" name="driverid" placeholder="Enter driver's ID" >
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="EmailId is required">
					<span class="label-input100">Driver's EmailId</span>
					<input class="input100" type="email " name="EmailId" placeholder="Enter EmailId" >
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Phone number is required">
					<span class="label-input100">Contact number</span>
					<input class="input100" type="tel" name="number" placeholder="Enter phone number" >
					<span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Phone number is required">
					<span class="label-input100">Create Password</span>
					<input class="input100" type="password" name="password" placeholder="Enter Password" >
					<span class="focus-input100"></span>
                </div>
			    

		

				<div class="container-contact100-form-btn">
					<div class="wrap-contact100-form-btn">
						<div class="contact100-form-bgbtn"></div>
						<button class="contact100-form-btn">
							<span>
								Submit
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
						</button>
					</div>
				</div>
			</form>
		</div>
	</div>
	
	<div id="dropDownSelect1"></div>
